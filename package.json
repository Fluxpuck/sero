{
  "name": "flux",
  "version": "1",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "workspaces": {
    "nohoist": [
      "module-alias"
    ],
    "packages": [
      "packages/*"
    ]
  },
  "_moduleAliases": {
    "@api": "./packages/api",
    "@bot": "./packages/bot"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "chalk": "^5.3.0",
    "globals": "^15.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "turbo": "^1.10.12"
  },
  "scripts": {
    "start-api": "npm run start-api --prefix ./packages/api",
    "start-bot": "npm run start-api --prefix ./packages/bot",
    "seedmanager": "node packages/api/test/_seedManager.js"
  }
}