    "version": "2.0.0",
    "configurations": [
        {
            "name": "Sero API",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/packages/api/app.js",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "node"
        },
        {
            "name": "Sero Bot",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/packages/bot/bot.js",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "node"
        },
        {
            "name": "Sero Agent",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/packages/agent/index.ts",
            "cwd": "${workspaceFolder}/packages/agent",
            "runtimeExecutable": "node",
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
        },
        {
            "name": "Seeder",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/packages/api/test/_seedManager.js",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "node"
        },
    ]
}