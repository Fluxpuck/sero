    "version": "2.0.0",
    "configurations": [
        {
            "name": "Sero API",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/packages/api/app.js",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "node"
        },
        {
            "name": "Sero Bot",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/packages/bot/bot.js",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "node"
        },
        {
            "name": "Sero Agent",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/packages/agent/index.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "ts-node"
        },
        {
            "name": "Seeder",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/packages/api/test/_seedManager.js",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "node"
        },
        {
            "name": "Sero API v2",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/packages/api-v2/main.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "ts-node",
            "outFiles": [
                "${workspaceFolder}/packages/api-v2/**/*.js"
            ]
        },
        {
            "name": "Seeder v2",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/packages/api-v2/database/seeds/index.ts",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "ts-node",
            "outFiles": [
                "${workspaceFolder}/packages/api-v2/**/*.js"
            ]
        }
    ]
}